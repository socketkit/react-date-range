{
  "name": "react-date-range",
  "version": "1.1.3",
  "description": "A React component for choosing dates and date ranges.",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build:css & styleguidist server",
    "build": "npm run build:css & npm run build:js & styleguidist build",
    "build:css": "postcss 'src/styles.scss' -d dist --ext css & postcss 'src/theme/*.scss' -d 'dist/theme' --ext css",
    "build:js": "babel ./src --out-dir ./dist --ignore test.js",
    "lint": "eslint 'src/**/*.js'",
    "test": "jest",
    "preversion": "npm run clear & npm run build"
  },
  "keywords": [
    "react",
    "date",
    "range",
    "datepicker",
    "rangepicker"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/Adphorus/react-date-range"
  },
  "bugs": {
    "url": "http://github.com/Adphorus/react-date-range/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "classnames": "^2.3.0",
    "prop-types": "^15.7.2",
    "react-list": "^0.8.16",
    "shallow-equal": "^1.2.1"
  },
  "peerDependencies": {
    "date-fns": "^2.19.0",
    "react": "^17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "date-fns": "^2.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "jest": "^26.6.3",
    "normalize.css": "^8.0.1",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^17.0.2",
    "react-styleguidist": "^11.1.6",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.30.0"
  },
  "prettier": {
    "printWidth": 110,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5",
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "printWidth": 100,
          "parser": "babylon"
        }
      ],
      "no-debugger": 0,
      "no-console": 0
    },
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jest": true
    }
  }
}
